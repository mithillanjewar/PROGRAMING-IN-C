#include <stdio.h>

// Function to copy a string
void stringCopy(char *dest, const char *src) {
    while (*src) {
        *dest = *src;
        dest++;
        src++;
    }
    *dest = '\0';
}

// Function to concatenate two strings
void stringConcat(char *dest, const char *src) {
    while (*dest) dest++;  // Move to end of dest
    while (*src) {
        *dest = *src;
        dest++;
        src++;
    }
    *dest = '\0';
}

// Function to compare two strings
int stringCompare(const char *str1, const char *str2) {
    while (*str1 && (*str1 == *str2)) {
        str1++;
        str2++;
    }
    return *(unsigned char *)str1 - *(unsigned char *)str2;
}

int main() {
    char str1[100], str2[100], result[200];

    printf("Enter first string: ");
    gets(str1);

    printf("Enter second string: ");
    gets(str2);

    // Copy
    stringCopy(result, str1);
    printf("\nAfter stringCopy, result = %s\n", result);

    // Concatenate
    stringConcat(result, str2);
    printf("After stringConcat, result = %s\n", result);

    // Compare
    int cmp = stringCompare(str1, str2);
    if (cmp == 0)
        printf("stringCompare: Strings are equal.\n");
    else if (cmp < 0)
        printf("stringCompare: str1 is less than str2.\n");
    else
        printf("stringCompare: str1 is greater than str2.\n");

    return 0;




}
